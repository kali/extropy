<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="rootLogger" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-8r [%t] %-5p %c - %m%n</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="rootLogger" />
	</root>
	<logger name="org.zoy.kali" level="INFO" />
	<logger name="org.zoy.kali.extropy.Rule" level="TRACE" />

</configuration>
